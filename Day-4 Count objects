const getCount = objects => objects.filter(o=> o.x===o.y).length;    /*one line solution*/

                       /*OR*/





function getCount(objects) {
    let c=0;
    for(let i=0;i<objects.length;i++){
        if (objects[i].x === objects[i].y){
            c++;
        }
    }
    return c
    
}
